#include <iostream>
using namespace std;
#define SIZE 5
int stack[SIZE];
int top=-1;

void push(int a){
    if(top==SIZE-1){
    cout<<"stack is overflow"<<endl;

    }else{
        top++;
        stack[top]=a;
        cout<<a<<" inserted"<<endl;
      
    }
}
void pop(){
    if(top==-1){
    cout<<"stack is underflow"<<endl;

}else{
    cout<<stack[top]<<" removed"<<endl;
    top--;
}
}

void display(){
    if(top==-1){
        cout<<"stack is empty"<<endl;
        
    }
    else{
        for(int i=top;i>=0;i--){
            cout<<stack[i]<<" ";
        }
    }
}
int main(){
    push(10);
    push(20);
    push(30);
    push(40);
    pop();
    pop();
    display();
    
    
}
